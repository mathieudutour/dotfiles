{
  // Place your settings in this file to overwrite the default settings
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.fontLigatures": false,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.rulers": [80],
  "editor.detectIndentation": false,
  "editor.wrappingIndent": "indent",
  "editor.dragAndDrop": false,
  "window.restoreFullscreen": true,
  "files.insertFinalNewline": true,
  "editor.folding": false,
  "editor.formatOnPaste": true,
  "editor.renderLineHighlight": "all",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.idea": true,
    "**/.svn": true,
    "**/node_modules": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/tmp": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "**/tmp": true
  },
  "files.trimTrailingWhitespace": true,
  "html.suggest.html5": true,
  "npm-intellisense.scanDevDependencies": true,
  "terminal.external.osxExec": "iTerm.app",
  "javascript.validate.enable": false,
  "extensions.autoUpdate": true,
  "workbench.colorTheme": "One Monokai",
  "workbench.iconTheme": "material-icon-theme",
  "git.confirmSync": false,
  "editor.scrollBeyondLastLine": false,
  "typescript.check.npmIsInstalled": false,
  "material-icon-theme.showUpdateMessage": false,
  "js/ts.implicitProjectConfig.checkJs": false,
  "guides.enabled": false,
  "workbench.startupEditor": "newUntitledFile",
  "extensions.ignoreRecommendations": true,
  "search.useIgnoreFiles": true,
  "workbench.fontAliasing": "antialiased",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "git.autofetch": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "beautify.ignore": ["**/*"],
  "editor.acceptSuggestionOnEnter": "off",
  "editor.accessibilitySupport": "off",
  "workbench.tips.enabled": false,
  "breadcrumbs.enabled": true,
  "workbench.enableExperiments": false,
  "keyboard.touchbar.enabled": false,
  "editor.cursorSmoothCaretAnimation": "on",
  "files.associations": {
    "*.component": "json",
    "*.zsh": "shellscript",
    "*.symlink": "shellscript",
    "Fastfile": "ruby",
    "Appfile": "ruby",
    "Matchfile": "ruby",
    "*.logic": "swift",
    "*.sb": "scheme"
  },
  "git.enableSmartCommit": true,
  "emojisense.unicodeCompletionsEnabled": false,
  "emojisense.languages": {
    "markdown": true,
    "plaintext": {
      "markupCompletionsEnabled": false,
      "emojiDecoratorsEnabled": false
    },
    "git-commit": true,
    "mdx": true
  },
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.showWelcomeMessage": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.statusBar.visible": true,
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.fontFamily": "'JetBrains Mono', Menlo, Monaco, 'Courier New', monospace",
  "diffEditor.ignoreTrimWhitespace": false,
  "gitlens.codeLens.scopes": ["document"],
  "gitlens.statusBar.enabled": false,
  "gitlens.hovers.avatars": false,
  "gitlens.hovers.annotations.enabled": false,
  "guides.sendUsagesAndStats": false,
  "typescript.surveys.enabled": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "spellright.language": ["en"],
  "spellright.documentTypes": ["markdown", "latex", "plaintext"],
  "emmet.showExpandedAbbreviation": "never",
  "git.enableCommitSigning": true,
  "redhat.telemetry.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false
  },
  "editor.bracketPairColorization.enabled": true,
  "go.toolsManagement.autoUpdate": true,
  "githubPullRequests.createOnPublishBranch": "never",
  "npm.keybindingsChangedWarningShown": true,
  "grammarly.files.include": [
    "**/readme.md",
    "**/README.md",
    "**/*.txt",
    "**/*.md"
  ],
  "grammarly.config.suggestions.OxfordComma": true,
  "githubPullRequests.pullBranch": "never",
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "git.rebaseWhenSync": true,
  "vscode-home-assistant.ignoreCertificates": true,
  "vscode-home-assistant.longLivedAccessToken": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJiMzQ4ZDA2NWQwN2U0Y2U4YmNlMzExYTY1ZTg2Nzg0NSIsImlhdCI6MTY3ODI4MjU2OCwiZXhwIjoxOTkzNjQyNTY4fQ.Ju3Yo8FoOPqZUawzFUjm8D9sPuf656CzOT3BWCrEHFA",
  "vscode-home-assistant.hostUrl": "http://homeassistant.local:8123",
  "editor.guides.bracketPairs": "active",
  "gitlens.autolinks": [
    {
      "prefix": "RAY-",
      "url": "https://linear.app/raycast/issue/RAY-<num>"
    }
  ],
  "arduino.useArduinoCli": true,
  "arduino.logLevel": "verbose",
  "arduino.disableIntelliSenseAutoGen": true,
  "arduino.openPDEFiletype": true,
  "implicitParentheses.enable": false,
  "githubPullRequests.terminalLinksHandler": "github",
  "github.copilot.editor.enableAutoCompletions": true,
  "gitlens.advanced.messages": {
    "suppressIntegrationRequestTimedOutWarning": true
  },
  "security.promptForLocalFileProtocolHandling": false,
  "application.shellEnvironmentResolutionTimeout": 100,
  "typescript.updateImportsOnFileMove.enabled": "always"
}
